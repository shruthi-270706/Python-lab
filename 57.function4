def perfect_without_parameters_with_return():
    number = int(input("Enter the number: "))
    sum = 0
    for i in range(1, number):
        if number % i == 0:
            sum += i
    return sum == number
def main():
    if perfect_without_parameters_with_return():
        print("Perfect number")
    else:
        print("Not a perfect number")
main()